{
	"name": "aacr-bold-panel-03",
	"version": "1.0.0",
	"description": "",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"pack-electron-windows": "electron-builder --win --x64",
		"pack-electron-mac": "electron-builder --mac",
		"distribute-windows": "npm-run-all -s pack-electron-windows",
		"distribute-mac": "npm-run-all -s pack-electron-mac"
	},
	"build": {
		"appId": "com.boldsci.aacrcongress",
		"artifactName": "AACR-Panel-03-${version}.${ext}",
		"directories": {
			"output": "./distribution/"
		},
		"files": [
			"dist/**/*",
			"main.js",
			"preload.js"
		],
		"asarUnpack": [
			"dist/**/*"
		],
		"win": {
			"target": "portable"
		},
		"mac": {
			"target": "dir",
			"category": "public.app-category.utilities"
		}
	},
	"repository": "",
	"keywords": [],
	"author": "BOLD Science",
	"license": "",
	"devDependencies": {
		"electron": "^27.0.3",
		"electron-builder": "^24.6.4"
	},
	"dependencies": {
		"browser-sync": "^2.29.3",
		"express": "^4.18.2",
		"http-server": "^14.1.1",
		"npm-run-all": "^4.1.5"
	}
}
