{
	"name": "aacr-congress-build",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"pre:microsite": "node pre-version.js microsite",
		"pre:kiosk1": "node pre-version.js kiosk1",
		"pre:kiosk2": "node pre-version.js kiosk2",
		"pre:kiosk3": "node pre-version.js kiosk3",
		"dev": "npm run pre:microsite && vite",
		"dev:kiosk1": "npm run pre:kiosk1 && vite",
		"dev:kiosk2": "npm run pre:kiosk2 && vite",
		"dev:kiosk3": "npm run pre:kiosk3 && vite",
		"build": "npm run pre:microsite && run-p type-check \"build-only {@}\" --",
		"build:kiosk1": "npm run pre:kiosk1 && run-p type-check \"build-only {@}\" --",
		"build:kiosk2": "npm run pre:kiosk2 && run-p type-check \"build-only {@}\" --",
		"build:kiosk3": "npm run pre:kiosk3 && run-p type-check \"build-only {@}\" --",
		"preview": "vite preview",
		"test:unit": "vitest",
		"build-only": "vite build",
		"type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"format": "prettier --write src/"
	},
	"dependencies": {
		"canvas": "^2.11.2",
		"gsap": "^3.12.5",
		"lodash": "^4.17.21",
		"path2d-polyfill": "^2.0.1",
		"pinia": "^2.1.7",
		"qrcode.vue": "^3.4.1",
		"vue": "^3.3.4",
		"vue-gtag": "^2.0.1",
		"vue-i18n": "^9.7.0",
		"vue-router": "^4.2.5",
		"vue3-carousel": "^0.3.1"
	},
	"devDependencies": {
		"@rushstack/eslint-patch": "^1.3.3",
		"@tsconfig/node18": "^18.2.2",
		"@types/jsdom": "^21.1.3",
		"@types/node": "^18.18.5",
		"@vitejs/plugin-vue": "^4.4.0",
		"@vue/eslint-config-prettier": "^8.0.0",
		"@vue/eslint-config-typescript": "^12.0.0",
		"@vue/test-utils": "^2.4.1",
		"@vue/tsconfig": "^0.4.0",
		"eslint": "^8.49.0",
		"eslint-plugin-vue": "^9.17.0",
		"jsdom": "^22.1.0",
		"npm-run-all2": "^6.1.1",
		"prettier": "^3.0.3",
		"sass": "^1.69.5",
		"typescript": "~5.2.0",
		"vite": "^4.4.11",
		"vitest": "^0.34.6",
		"vue-tsc": "^1.8.19"
	}
}
